.contact-icon:link,.contact-icon:visited{text-decoration:none;color:#393e46;font-size:.6em;text-transform:uppercase;letter-spacing:.1em}.contacts-form{border:8px dashed #f0f0f0;padding:1em 2em}.contacts-form h3{margin:0}.contacts-form label{font-size:.6em;text-transform:uppercase;letter-spacing:.2em;display:block;width:100%}.contacts-form input[type=email],.contacts-form input[type=text],.contacts-form textarea{display:block;width:98%;background:#f0f0f0;border:none;padding:1em;color:#67707e}.contacts-form input[type=email]:focus,.contacts-form input[type=text]:focus,.contacts-form textarea:focus{outline:none}.contacts-form .one-half{width:48%;margin-right:4%;float:left}.contacts-form .one-third{width:30%;margin-right:calc(10%/2);float:left}.contacts-form .one-fourth{width:24%;margin-right:calc(4%/3);float:left}.contacts-form .last{margin-right:0}